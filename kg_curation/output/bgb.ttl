    bgb-onto:hasParagraph bgb-data:norm_1001_para_1 ;
    bgb-onto:isRepealed false ;
    bgb-onto:normIdentifier " 1001" ;
    dcterms:title " 1001 Klage auf Verwendungsersatz" .

