    dcterms:title " 1041 Erhaltung der Sache" .

bgb-data:norm_1043 a bgb-onto:Norm ;
    bgb-onto:hasParagraph bgb-data:norm_1043_para_1 ;
    bgb-onto:isRepealed false ;
